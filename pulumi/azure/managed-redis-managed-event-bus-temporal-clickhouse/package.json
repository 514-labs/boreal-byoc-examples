{
  "name": "byoc-example-azure-managed-services",
  "version": "1.0.0",
  "description": "Pulumi project for Azure managed services with Redis, Event Hubs, Temporal, and ClickHouse",
  "main": "src/index.ts",
  "scripts": {
    "deploy": "npx tsx launcher.ts up --org ${ORG}",
    "deploy:all": "npx tsx launcher.ts up --org ${ORG}",
    "deploy:base": "npx tsx launcher.ts up --org ${ORG} --stack base",
    "deploy:byoc": "npx tsx launcher.ts up --org ${ORG} --stack byoc-services",
    "deploy:datadog": "npx tsx launcher.ts up --org ${ORG} --stack datadog",
    "deploy:mds": "npx tsx launcher.ts up --org ${ORG} --stack mds",
    "destroy:all": "npx tsx launcher.ts destroy --org ${ORG}",
    "destroy:base": "npx tsx launcher.ts destroy --org ${ORG} --stack base",
    "destroy:byoc": "npx tsx launcher.ts destroy --org ${ORG} --stack byoc-services",
    "destroy:datadog": "npx tsx launcher.ts destroy --org ${ORG} --stack datadog",
    "destroy:mds": "npx tsx launcher.ts destroy --org ${ORG} --stack mds",
    "preview:all": "npx tsx launcher.ts preview --org ${ORG}",
    "preview:base": "npx tsx launcher.ts preview --org ${ORG} --stack base",
    "preview:byoc": "npx tsx launcher.ts preview --org ${ORG} --stack byoc-services",
    "preview:datadog": "npx tsx launcher.ts preview --org ${ORG} --stack datadog",
    "preview:mds": "npx tsx launcher.ts preview --org ${ORG} --stack mds",
    "stack:select": "pulumi stack select",
    "stack:list": "pulumi stack ls",
    "create-config": "npx tsx create-config.ts"
  },
  "dependencies": {
    "@pulumi/azure-native": "catalog:",
    "@pulumi/command": "catalog:",
    "@pulumi/kubernetes": "catalog:",
